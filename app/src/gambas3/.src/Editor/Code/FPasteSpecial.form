# Gambas Form File 3.0

{ Form Form
  MoveScaled(0,0,68,50)
  Text = ("Paste special")
  Icon = Picture["icon:/small/paste"]
  Resizable = False
  Spacing = True
  Padding = 8
  { lstFormat ListBox
    MoveScaled(1,5,66,17)
  }
  { Panel2 Panel
    MoveScaled(1,23,66,20)
    Arrangement = Arrange.Vertical
    { radPasteNormal RadioButton
      MoveScaled(0,0,40,4)
      Text = ("Paste normally")
      Value = True
    }
    { radPasteComment RadioButton
      MoveScaled(0,4,40,4)
      Text = ("Paste as comments")
    }
    { radPastePrint RadioButton
      MoveScaled(0,8,40,4)
      Text = ("Paste with PRINT")
    }
    { radPasteString RadioButton
      MoveScaled(0,12,40,4)
      Text = ("Paste as string")
    }
    { radPasteStringSplit RadioButton
      MoveScaled(0,16,44,4)
      Text = ("Paste as multi-line string")
    }
  }
  { HBox1 HBox
    MoveScaled(1,45,66,4)
    Spacing = True
    { Panel1 Panel
      MoveScaled(1,0,4,4)
      Expand = True
    }
    { btnOK Button
      MoveScaled(15,0,14,4)
      Text = ("OK")
      Default = True
    }
    { btnCancel Button
      MoveScaled(30,0,14,4)
      Text = ("Cancel")
      Cancel = True
    }
  }
  { Label1 Label
    MoveScaled(1,1,48,4)
    Font = Font["Bold"]
    Text = ("Clipboard contents")
  }
}
